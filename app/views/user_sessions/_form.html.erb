<%= form_with url: user_sessions_path, method: :post do |form| %>
  <div class="field">
    <%= form.label :email %><br />
    <%= form.text_field :email %>
  </div>
  <div class="field">
    <%= form.label :password %><br />
    <%= form.password_field :password %>
  </div>
  <div class="actions">
    <%= form.submit "Login" %>
  </div>
<% end %>
